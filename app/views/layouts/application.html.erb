<!DOCTYPE html>
<html>
<head>
  <title>ScreenerTrack</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>

</head>


<body>
	<div id="container">
	<%= render '/common/header'%>

	<% flash.each do |name, msg| %>
	  <%= content_tag :div, msg, :id => "flash_#{name}" %>
	<% end %>

<%= yield %>

	<%= render '/common/footer'%>
	</div>
</body>
</html>
