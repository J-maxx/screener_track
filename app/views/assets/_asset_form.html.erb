<%= simple_form_for @asset do |f| %>
	<%= f.association :project, :include_blank => false,  :default=>@asset.project %> 
	<%= f.input :version_name %>
	<%= f.input :running_time  %>
	<%= f.input :media %>
	<%= f.input :cut_description %>
	<%= f.submit (@asset.new_record? ? "Create Asset" : "Update Asset" ) %>
<% end %>